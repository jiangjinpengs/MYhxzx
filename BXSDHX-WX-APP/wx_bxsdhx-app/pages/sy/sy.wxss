/* pages/sy/sy.wxss */

.header {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
}

.header>input {
  width: 700rpx;
  height: 50rpx;
  font-size: 26rpx;
  border: 1rpx solid #999;
  text-align: center;
  border-radius: 20rpx;
}

.headerlist {
  width: 100%;
  height: 200rpx;
}

.headerlist image {
  width: 160rpx;
  height: 160rpx;
}

.headerlist-list {
  width: 25%;
}

.headerlist-list>view {
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
}

.daohang {
  display: flex;
  flex-direction: column;
  padding-top: 50rpx;
  padding-bottom: 40rpx;
}

/* pages/home/home.wxss */

.inholde {
  display: flex;
  background-color: #f5f5f5;
  height: 120rpx;
  align-items: center;
  padding-left: 18rpx;
  padding-right: 18rpx;
}

.inleo {
  width: 720rpx;
  height: 50rpx;
  background: white;
  text-align: center;
  font-size: 30rpx;
  border-radius: 20rpx;
}

.daohang {
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
  padding-bottom: 40rpx;
}

.youhuj {
  background: #efeff4;
  height: 372rpx;
  position: relative;
}

.youhuj1s {
  height: 350rpx;
  width: 100%;
  background: white;
  position: absolute;
  top: 10rpx;
}

.tishi {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  padding-left: 18rpx;
  padding-right: 18rpx;
}

.gundong {
  height: 50rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
}

.xinwen {
  margin-bottom: 30rpx;
}

.footer {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  font-size: 26rpx;
  color: #999;
  background-color: #f2f2f2;
}

.Rankings {
  position: fixed;
  top: 20rpx;
  left: 2.3%;
  width: 720rpx;
  height: 1000rpx;
  background-color: black;
  z-index: 1000;
  overflow-y: scroll;
  overflow-x: hidden;
  border-radius: 10rpx;
}

.Rankings-no {
  display: inline-block;
  font-size: 60rpx;
  color: #9c9c9c;
  margin-top: 20rpx;
  margin-left: 20rpx;
  padding: 10rpx;
  background-color: black;
  border-radius: 50%;
  opacity: 1;
}

.Rankings-myuser {
  width: 100%;
  margin-top: 10rpx;
  background-color: black;
  height: 200rpx;
  display: flex;
  align-items: center;
}

.Rankings-jzgd {
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 22rpx;
  height: 60rpx;
}

.Rankings-myuser-img {
  width: 200rpx;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.Rankings-myuser-img >image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}

.Rankings-myuser-text {
  font-size: 28rpx;
  color: white;
}

.Rankings-list {
  width: 98%;
  margin: 0 auto;
  font-size: 24rpx;
  color: white;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-top: 8rpx;
  border-top: 1px solid rgb(204, 204, 204);
}

.Rankings-list image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
